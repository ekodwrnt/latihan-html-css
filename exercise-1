//Newton Second Law
STORE "M" = 600
STORE "A" = 2
STORE "ΣF" without any value

CALCULATE "M" times "A"
SET "ΣF" value with calculation result
DISPLAY "ΣF"

//Tahun Kabisat
STORE "year" with "number"
IF "year" mod 4 equal to 0 THEN
   IF "year" mod 100 not equal to 0 
    ELSE IF "year" mod 4 equal to 0, THEN
        IF "year" mod 100 equal to 0 THEN
          IF "year" mod 400 equal to 0
  DISPLAY "'year' adalah tahun kabisat"
    ELSE
   DISPLAY "'year' bukan tahun kabisat"
DISPLAY "year"

//Laundry Day
STORE "Laundry" with 0
WHILE "Laundry" <= 20
  ADD "Laundry" by 1
DISPLAY "Laundry process"  

//Periksa Kuku
STORE "count" to 1
WHILE "count" < 41
  ADD "count" by 1
DISPLAY "count"

STORE "Kuku Pendek" with "count mod 2 equal to 0"
STORE "Kuku Panjang" with "count mod 2 not equal to 0"
IF "count" mod 2 equal to 0
DISPLAY "Guru akan memuji siswa/siswi"
ELSE
DISPLAY "Guru akan menghukum siswa/siswi"
